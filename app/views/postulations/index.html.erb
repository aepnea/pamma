<p id="notice"><%= notice %></p>

<h1>Listing Postulations</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Asociative users</th>
      <th>Personal backgrounds</th>
      <th>Family group</th>
      <th>Pamma participation</th>
      <th>Beneficiary</th>
      <th>Characterization work</th>
      <th>Benefit requested</th>
      <th>Representative</th>
      <th>Milestone name</th>
      <th>Milestone description</th>
      <th>Milestone date begin</th>
      <th>Milestone date end</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @postulations.each do |postulation| %>
      <tr>
        <td><%= postulation.user_id %></td>
        <td><%= postulation.asociative_users_id %></td>
        <td><%= postulation.personal_backgrounds_id %></td>
        <td><%= postulation.family_group_id %></td>
        <td><%= postulation.pamma_participation_id %></td>
        <td><%= postulation.beneficiary_id %></td>
        <td><%= postulation.characterization_work_id %></td>
        <td><%= postulation.benefit_requested_id %></td>
        <td><%= postulation.representative_id %></td>
        <td><%= postulation.milestone_name %></td>
        <td><%= postulation.milestone_description %></td>
        <td><%= postulation.milestone_date_begin %></td>
        <td><%= postulation.milestone_date_end %></td>
        <td><%= link_to 'Show', postulation %></td>
        <td><%= link_to 'Edit', edit_postulation_path(postulation) %></td>
        <td><%= link_to 'Destroy', postulation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Postulation', new_postulation_path %>
