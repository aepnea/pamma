<div class="row">
  <div class="col-md-12">
    <%= form_for(@pamma_participation) do |f| %>
      <% if @pamma_participation.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@pamma_participation.errors.count, "error") %> prohibited this pamma_participation from being saved:</h2>

          <ul>
          <% @pamma_participation.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
          </ul>
        </div>
      <% end %>
      <div class="inner-file">
        <div class="header-file">
          <div class="col-md-8 center">
            <div class="header-cont">
              <div class="pull-left">
                <%= link_to pamma_participations_path do %>
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16.47 15.85"><defs><style>.cls-1{fill:#fff;}</style></defs><title>left</title><g id="Layer_2" data-name="Layer 2"><g id="Layer_2-2" data-name="Layer 2"><path class="cls-1" d="M5.76,2.17.07,7.77a.22.22,0,0,0,0,.31l5.69,5.6A.13.13,0,0,0,6,13.6V10c0-.16-.07-.25-.22-.4L4.17,8c-.07-.07-.07-.11,0-.18L5.76,6.27C5.92,6.12,6,6,6,5.87V2.26a.13.13,0,0,0-.22-.09"/><path class="cls-1" d="M16.24,0,8.43,7.77a.22.22,0,0,0,0,.31l7.81,7.72a.13.13,0,0,0,.22-.09V12.1c0-.16-.07-.25-.22-.4L12.54,8c-.07-.07-.07-.11,0-.18l3.71-3.68c.16-.16.22-.25.22-.4V.14A.13.13,0,0,0,16.24,0"/></g></g></svg>
                  volver
                <% end %>
              </div>
              <div class="pull-right">
                <div class="file-text">
                  <h3>Beneficios Anteriores</h3>
                  <p>Ac√° debes ingresar los tipos de beneficio que has recibido</p>
                </div>
                <div class="file-count"><p>02</p></div>
              </div>
            </div>
          </div>
        </div>
        <div class="body-form">
          <div class="col-md-9 center new-form">
            <div class="field4">
              <%= f.label "Nombre Beneficiario " %><br>
              <%= f.text_field :previous_beneficiary %>
            </div>
            <div class="field6">
              <%= f.label "Numero de veces" %><br>
              <%= f.number_field :number_times%>
            </div>
            <div class="field5">
              <%= f.label "Tipo de beneficio" %><br>
              <%= f.collection_select :benefit_type_id, BenefitType.all, :id, :name %>
            </div>
            <div class="field5">
              <%= f.label "Estado del proyecto" %><br>
              <%= f.collection_select :project_state_type_id, ProjectStateType.all, :id, :name %>
            </div>
            <div class="field5">
              <%= f.label "Tipo de Ayuda" %><br>
              <%= f.collection_select :support_type_id, SupportType.all, :id, :name %>
            </div>
            <div>
              <%= f.submit "Agregar", class: "create"  %>
            </div>
            <%if user_signed_in?%>
              <div class="field">
                <!-- para usuario normal -->
                <%= f.hidden_field :user_id, value: "#{current_user.id}" %>
              </div>
            <%else%>
              <%if asociative_user_signed_in?%>
                <div class="field">
                  <!-- para usuario asociativo-->
                  <%= f.hidden_field :asociative_user_id, value: "#{current_asociative_user.id}" %>
                </div>
              <%end%>
            <%end%>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
      
